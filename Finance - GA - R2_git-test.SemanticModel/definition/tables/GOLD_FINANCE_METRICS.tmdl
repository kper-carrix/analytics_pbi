			VAR MeasureSelectionTime = ALLSELECTED('Measure Selection Time Comparison'[Measure Selection])
			VAR MeasureSelectionProRata = ALLSELECTED('Measure Selection ProRata_Budget'[Measure Selection])
			
			RETURN
			SWITCH(TRUE(),
			    MeasureSelectionTime = "Budget" && MeasureSelectionProRata = "Gross", [Direct Cost Budget],
